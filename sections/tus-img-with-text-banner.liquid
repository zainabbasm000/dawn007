// $('.ab_tus_big_img_text img').css("opacity","0");

// setTimeout(function() {
// $('.ab_tus_big_img_text img').css("opacity","1");
// }, 1000);
// });


</script>


{% schema %}
{
"name": "c images text",
"settings": [
{
"type":"image_picker",
"id":"sec_bg_img",
"label":"Background Image(desktop)"  
},
{
"type":"image_picker",
"id":"sec_bg_img_mob",
"label":"Background Image(mobile)"  
},  
{
"type":"color",
"id":"sec_cont_bg",
"label":"Content background color",
"default":"#fff"  
},
{
"type":"richtext",
"id":"sec_heading",
"label":"Heading"  
},
{
"type":"textarea",
"id":"sec_content",
"label":"Text",
"default":"Find your perfect piece of King’s memorabilia to make sure you take that little bit of King’s with you wherever your journey takes you "    
},
{
"type":"color",
"id":"sec_text_color",
"label":"Text color",
"default":"#000"  
}  
],
"blocks": [
{
"type": "tus_sec_logo",
"name": "Logo image",
"settings": [
{
"type": "image_picker",
"id": "logo_img_block",
"label": "Logo image"
},
{
"type": "range",
"id": "logo_width",
"min": 30,
"max": 100,
"step": 1,
"unit": "px",
"label": "Logo width(desktop)",
"default": 50
}, 
{
"type": "range",
"id": "mob_log_width",
"min": 20,
"max": 60,
"step": 1,
"unit": "px",
"label": "Logo width(mobile)",
"default": 40
},  
{
"type":"url",
"id":"logo_link",
"label":"Logo link"  
}  
]
}  
],   
"presets":[
{
"name":"c images text",
"category":"office work"  
}  
]    
}
{% endschema %}

{% stylesheet %}
{% endstylesheet %}

{% javascript %}
{% endjavascript %}